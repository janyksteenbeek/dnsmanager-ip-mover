    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Janyk Steenbeek",
            "email": "info@janyksteenbeek.nl"
        }
    ],
    "autoload": {
        "psr-4": {"JanykSteenbeek\\DNSManager\\Mover\\": "src/"}
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes ."
    }
}
